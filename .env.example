# Copy this file to .env and fill the values before running
# Example .env for Ghost + MariaDB

# Public URL for your Ghost site (include protocol, no trailing slash)
SITE_URL=http://localhost:2368

# MariaDB / MySQL credentials used by Ghost
MYSQL_ROOT_PASSWORD=change_me_root_password
MYSQL_DATABASE=ghost
MYSQL_USER=ghost
MYSQL_PASSWORD=change_me_db_password
